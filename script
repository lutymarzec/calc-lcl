const volumeDropdown = document.getElementById("volume");
const departureDropdown = document.getElementById("departure");
const destinationDropdown = document.getElementById("destination");
const resultList = document.getElementById("result-list");

function updateResultsList() {
  const volume = volumeDropdown.value;
  const departure = departureDropdown.value;
  const destination = destinationDropdown.value;

  const priceRangeElement = document.getElementById("price-range");
  const doorToDoorChargeElement = document.getElementById(
    "door-to-door-charge"
  );
  const overallChargeRMBElement = document.getElementById("overall-charge-rmb");
  const overallChargeNZDElement = document.getElementById("overall-charge-nzd");

  if (
    volume &&
    departure &&
    destination &&
    prices[volume] &&
    prices[volume][departure] &&
    prices[volume][departure][destination]
  ) {
    const priceInfo = prices[volume][departure][destination];

    // Display the results list
    resultList.classList.remove("hidden");

    // Update the elements with the corresponding data
    priceRangeElement.textContent = priceInfo["Price Range"];
    doorToDoorChargeElement.textContent = priceInfo["Door-to-Door Charge"];
    overallChargeRMBElement.textContent = priceInfo["Overall Charge (RMB)"];
    overallChargeNZDElement.textContent = priceInfo["Overall Charge (NZD)"];
  } else {
    // Hide the results list if any pickable option is not set
    resultList.classList.add("hidden");
  }
}

volumeDropdown.addEventListener("change", updateResultsList);
departureDropdown.addEventListener("change", updateResultsList);
destinationDropdown.addEventListener("change", updateResultsList);
